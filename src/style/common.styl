// reset
*
  user-select: none
a
  text-decoration: none
  color inherit
.page
  background-color #f2f2f2

.d-flex
  display flex
  justify-content center
  align-items center
.justify-content-around
  justify-content: space-around
.justify-content-between
  justify-content: space-between

.p0
  padding 0
.p1
  padding 0.5rem

.p2
  padding 1rem
.p3
  padding 1.5rem
.p4
  padding 2rem

